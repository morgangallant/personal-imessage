{
  "handlers": [
    {
      "name": "Auto-Hello",
      "path": "hello.js",
      "triggers": ["Hello", "Hey", "Sup"],
      "threshold": 0.65
    },
    {
      "name": "Auto-Thanks",
      "path": "thanks.js",
      "triggers": [
        "Thanks!",
        "Appreciate it!",
        "You're the best, thanks!",
        "Thank you!",
        "Thanks xoxo",
        "Thx",
        "Thx bb"
      ],
      "threshold": 0.65
    },
    {
      "name": "Fallback",
      "path": "noop.js",
      "triggers": [],
      "fallback": true
    }
  ]
}
